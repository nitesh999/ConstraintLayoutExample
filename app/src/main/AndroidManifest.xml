<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.constraintlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BarrierConstraintActivity"></activity>
        <activity android:name=".GroupConstraintActivity" />
        <activity android:name=".ChainStyleWeightActivity" />
        <activity android:name=".AspectRationConstraintActivity" />
        <activity android:name=".PercentageConstraintWidthActivity" />
        <activity android:name=".dp0ConstraintActivity" />
        <activity android:name=".BiasAndMarginGoneConstraintActivity" />
        <activity android:name=".CenterToViewConstraintActivity" />
        <activity android:name=".BaselineConstraintActivity" />
        <activity android:name=".BasicConstraintActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>